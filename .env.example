AWS_ACCESS_KEY_ID=secret
AWS_DEFAULT_REGION=eu-west-1
AWS_SECRET_ACCESS_KEY=secret

CONTAINER_NAME=copilot-cli

# Github app permissions in Repository permissions: Administration:Read and write / Codespaces metadata: Read-only / Contents: Read and write / Packages: Read and write / Workflows: Read and write
GITHUB_APP_ID=secret
GITHUB_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----
secret
-----END RSA PRIVATE KEY-----"
GITHUB_INSTALLATION_ID=secret

PHOENIX_ENDPOINT="https://phoenix.example.com"
PHOENIX_API_KEY="secret"

SSH_PRIVATE_KEY="-----BEGIN OPENSSH PRIVATE KEY-----
secret
-----END OPENSSH PRIVATE KEY-----"

VOLUME_CONFIG=./config
VOLUME_WORKSPACE=./workspace

OLLAMA_HOST="http://ollama:11434"
OLLAMA_MODEL="qwen:0.5b"

# Auto-approve ALL commands during Copilot CLI session (YOLO mode)
# Set to "true" to enable automatic approval without interactive prompts for ANY command
# Set to "false" or leave unset to use default interactive behavior
# WARNING: Only enable in trusted, sandboxed environments. This bypasses all command approval dialogs.
COPILOT_AUTO_APPROVE_COMMANDS=false
